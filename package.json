{
  "name": "litchi-react-moble",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --progress --env env=development",
    "build": "webpack --progress --env env=production",
    "build:gray": "webpack --progress --env env=gray",
    "build:version": "cross-env IS_VERSION=true npm run build",
    "build:analyze": "webpack --progress --env env=production --analyze",
    "build:watch": "webpack --progress  --env env=production --watch",
    "stylelint": "stylelint \"src/**/*.{css,less,styl,scss,sass}\"",
    "stylelint:fix": "stylelint --fix \"src/**/*.{css,less,styl,scss,sass}\"",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix src --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --coverage --watchAll",
    "test:clean": "rimraf ./coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{css,less,scss,sass,styl}": [
      "stylelint --fix",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "jest --findRelatedTests",
      "git add"
    ]
  },
